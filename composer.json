{
    "name": "kodeeo/settings",
    "description": "Kodeeo Settings is persistent settings in laravel",
    "type": "package",
    "keywords": ["settings", "laravel", "lumen"],
    "homepage": "http://kodeeo.com",
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kodeeo",
            "email": "info@kodeeo.com"
        },
        {
            "name": "Shipu Ahamed",
            "email": "shipu@kodeeo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kodeeo\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodeeo\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodeeo\\Settings\\Providers\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
